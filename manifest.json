{
  "name": "Hello Extensions",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_icon": {
      "16": "lemon.png",
      "32": "lemon.png",
      "48": "lemon.png",
      "128": "lemon.png"
    }
  },
  "web_accessible_resources": [
    {
      "resources": ["changeVolume.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "background": {
    "service_worker": "main.js"
  },
  "permissions": ["activeTab", "scripting"]
}
